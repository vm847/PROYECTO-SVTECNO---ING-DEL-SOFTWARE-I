	<queryString>
		<![CDATA[SELECT cc.fecha_inicio_credito, cl.cod_cliente, CONCAT(cl.nombre_cliente,' ',cl.apellido_cliente), 
sum(cdc.monto_cuota), vc.cod_venta_cabecera, vc.usuario_insercion FROM cuotas_credito_cliente cdc
JOIN credito_cliente cc ON cdc.cod_credito_cliente = cc.cod_credito_cliente
JOIN ventas_cabecera vc ON cc.cod_venta = vc.cod_venta_cabecera
JOIN clientes cl ON vc.cod_cliente = cl.cod_cliente
WHERE estado_cuota='ACTIVO' AND vc.fecha_venta >= $P{fecha_desde}  AND vc.fecha_venta <= $P{fecha_hasta}  
GROUP BY cc.cod_credito_cliente]]>
