	<queryString>
		<![CDATA[SELECT
pr.cod_presupuesto,
CONCAT(pr.cod_cliente,' - ', cl.nombre_cliente) AS cliente,
pr.fecha_presupuesto,
pr.estado_presupuesto,
COALESCE(pr.num_facturacion, '') AS facturacion, 
SUM(pd.precio_producto*pd.cantidad_producto) AS total
FROM presupuestos pr
JOIN clientes cl ON pr.cod_cliente=cl.cod_cliente
JOIN presupuesto_detalle pd ON pr.cod_presupuesto=pd.cod_presupuesto_cabecera
WHERE pr.fecha_presupuesto >=  $P{fecha_desde} AND pr.fecha_presupuesto <=  $P{fecha_hasta}
group by pr.cod_presupuesto]]>
	</queryString>
