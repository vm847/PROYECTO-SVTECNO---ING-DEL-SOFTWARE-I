	<queryString>
		<![CDATA[SELECT
 vc.cod_venta_cabecera,
 vc.razon_social_cliente,  
 vc.ruc_cliente,
 COALESCE(vc.tipo_movimiento, '') AS tipo,
 vc.fecha_venta, 
COALESCE( vc.num_comprobante_venta,'') AS comprobante,
COALESCE( vc.concepto_movimiento,'') AS concepto, 
COALESCE( (
SELECT
SUM(mp.monto_venta)
FROM metodo_pago_venta mp
WHERE mp.cod_venta = vc.cod_venta_cabecera
),'') AS monto,
vc.usuario_insercion
FROM ventas_cabecera vc	
join clientes cl on vc.cod_cliente = cl.cod_cliente
WHERE vc.fecha_venta >=  $P{fecha_desde}  AND vc.fecha_venta <=  $P{fecha_hasta}  AND vc.estado_venta= $P{estado_venta}]]>
	</queryString>
