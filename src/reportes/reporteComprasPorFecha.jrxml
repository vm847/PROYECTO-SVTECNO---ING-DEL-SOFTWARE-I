	<queryString>
		<![CDATA[SELECT 
cc.cod_compra_cabecera, 
cc.nom_proveedor, 
cc.ruc_proveedor, 
cc.fecha_comprobante, 
COALESCE(cc.num_comprobante_compra,''), 
cc.tipo_movimiento, 
COALESCE(
	(
	SELECT SUM(cd.cant_producto*cd.prec_producto) 
	FROM compras_detalle cd 
	WHERE cd.cod_compra_cabecera=cc.cod_compra_cabecera
	)
,
	(
		SELECT monto_cuota FROM cuotas_credito_proveedor WHERE num_movimiento= cc.cod_compra_cabecera
	)
) AS monto, 
CC.usuario_insercion
FROM compras_cabecera cc 	
WHERE cc.fecha_comprobante >=  $P{fecha_desde}  AND cc.fecha_comprobante <=  $P{fecha_hasta}  AND cc.estado_compra= $P{estado_compra}]]>
	</queryString>
